<Window x:Class="Pos_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pos_Client"
        mc:Ignorable="d" Title="Client"
        Height="450" Width="800">
    <Grid>
        <!--
        Frame : Window 내 Page 를 표시하기 위함
        NavigationUIVisibility : 상단 네비게이션 바(뒤로가기/앞으로가기 등의 버튼을 포함함) 표시 여부. 
        -->
        <Frame x:Name="main_frame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
